{
   "name": "Batman Mono",
   "type": "dark",
   "colors": {
      "focusBorder": "#38392955",
      "editorCursor.foreground": "#e83551",
      "editorCursor.background": "#eeffff",
      "editorLineNumber.foreground": "#424242",
      "editor.background": "#131413",
      "editor.foreground": "#eeffff",
      "scrollbarSlider.activeBackground": "#c0bd81",
      "scrollbarSlider.hoverBackground": "#383929",
      "titleBar.activeBackground": "#0c0d0c",
      "statusBar.noFolderBackground": "#0c0d0c",
      "statusBar.noFolderBorder": "#00000000",
      "statusBar.foreground": "#919191",
      "statusBarItem.hoverBackground": "#383929",
      "statusBar.background": "#090905",
      "activityBar.background": "#090905",
      "activityBar.border": "#060a06",
      "activityBar.activeBorder": "#c0bd81",
      "activityBarBadge.background": "#c0bd81",
      "activityBarBadge.foreground": "#0c0d0c",
      "tab.border": "#0c0d0c",
      "tab.inactiveBackground": "#131413",
      "tab.activeBackground": "#131413",
      "tab.activeForeground": "#919188",
      "tab.activeBorder": "#c0bd81",
      "breadcrumb.foreground": "#919191",
      "sideBar.background": "#0c0c08",
      "sideBar.foreground": "#919191",
      "terminal.tab.activeBorder": "#c0bd81",
      "terminal.foreground": "#919191",
      "panel.background": "#0c0d0c",
      "panel.border": "#060a06",
      "panelTitle.activeBorder": "#c0bd81",
      "panelTitle.activeForeground": "#ddddcc",
      "list.inactiveSelectionBackground": "#191915",
      "list.inactiveSelectionForeground": "#ddddcc",
      "list.activeSelectionBackground": "#292922",
      "list.activeSelectionForeground": "#ddddcc",
      "list.hoverBackground": "#191915",
      "list.hoverForeground": "#ddddcc",
      "list.focusOutline": "#00000000",
      "sideBarSectionHeader.background": "#0c0d0c",
      "sideBarSectionHeader.foreground": "#5a5a5a",
      "sideBar.border": "#060a06"
   },
   "tokenColors": [
      {
         "name": "Comment",
         "scope": ["comment", "punctuation.definition.comment"],
         "settings": {
            "fontStyle": "italic",
            "foreground": "#424242"
         }
      },
      {
         "name": "Variables",
         "scope": ["variable", "string constant.other.placeholder"],
         "settings": {
            "foreground": "#EEFFFF"
         }
      },
      {
         "name": "Keyword, Storage",
         "scope": ["keyword", "storage.type", "storage.modifier"],
         "settings": {
            "foreground": "#919191"
         }
      },
      {
         "name": "String, Symbols, Inherited Class, Markup Heading",
         "scope": [
            "string",
            "constant.other.symbol",
            "constant.other.key",
            "entity.other.inherited-class",
            "markup.heading",
            "markup.inserted.git_gutter",
            "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
         ],
         "settings": {
            "foreground": "#FFE181"
         }
      },
      {
         "name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
         "scope": [
            "constant.numeric",
            "constant.language",
            "support.constant",
            "constant.character",
            "constant.escape",
            "variable.parameter",
            "keyword.other.unit",
            "keyword.other"
         ],
         "settings": {
            "foreground": "#B3ABE4"
         }
      },
      {
         "name": "Primitive Types",
         "scope": [
            "meta.type.annotation.ts",
            "support.type.property-name.json",
            "entity.name.tag.html",
            "entity.name.tag.yaml",
            "meta.function-call.generic.python",
            "entity.name.function",
            "support.function.any-method"
         ],
         "settings": {
            "foreground": "#bcbcbc"
         }
      },
      {
         "name": "Paranthesis",
         "scope": [
            "punctuation.definition.block.begin",
            "punctuation.definition.block.end",
            "punctuation.definition.arguments.begin",
            "punctuation.definition.arguments.end",
            "punctuation.definition.parameters.begin",
            "punctuation.definition.parameters.end",
            "punctuation.definition.dict.begin",
            "punctuation.definition.dict.end",
            "punctuation.definition.list.begin",
            "punctuation.definition.list.end",
            "punctuation.section.class.begin.bracket.curly",
            "punctuation.section.class.end.bracket.curly",
            "punctuation.section.method.begin.bracket.curly",
            "punctuation.section.method.end.bracket.curly",
            "punctuation.bracket.square",
            "punctuation.bracket.round",
            "punctuation.bracket.curly",
            "punctuation.brackets.square",
            "punctuation.brackets.round",
            "punctuation.brackets.curly",
            // "punctuation.definition.block.ts",
            "meta.brace.square",
            "meta.brace.round",
            "meta.brace.curly",
            "punctuation.section.property-list.begin.bracket.curly.css",
            "punctuation.section.property-list.end.bracket.curly.css",
            "punctuation.section.function.begin.bracket.round.css",
            "punctuation.section.function.end.bracket.round.css",
            "punctuation.definition.entity.begin.bracket.square.css",
            "punctuation.definition.entity.end.bracket.square.css",
            "meta.parameters.ts",
            "meta.arrow.ts",
            "storage.type.annotation",
            "punctuation.definition.annotation",
            "entity.name.function.decorator"
            // "meta.structure.dictionary.json",
            // "punctuation.definition.dictionary.begin.json",
            // "punctuation.definition.dictionary.end.json"
         ],
         "settings": {
            // "foreground": "#fde2a1"
            "foreground": "#919191"
         }
      },
      {
         "name": "Operator, Misc",
         "scope": [
            "keyword.operator.arrow",
            "keyword.operator.custom.infix",
            "keyword.operator.assignment",
            "keyword.operator.arithmetic",
            "keyword.operator.increment-decrement",
            "storage.type.function.arrow",
            "keyword.operator.comparison",
            "keyword.operator.relational"
         ],
         "settings": {
            "foreground": "#89DDFF"
         }
      }

